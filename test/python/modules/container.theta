language-version: 1.0.0
avro-version: 1.0.0
---

// A record that has a field for every primitive type supported in
// Theta.
type ContainerRecord = {
  bool     : Bool,
  bytes    : Bytes,
  int      : Int,
  long     : Long,
  float    : Float,
  double   : Double,
  string   : String,
  date     : Date,
  datetime : Datetime
}
